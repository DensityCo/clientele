{
  "variables": {
    "core": "https://api.density.io/v1"
  },
  "resources": {
    "spaces": {
      "list": {
        "method": "GET",
        "url": "{{{core}}}/spaces/"
      },
      "get": {
        "method": "GET",
        "url": "{{{core}}}/spaces/{{id}}/"
      },
      "count": {
        "method": "GET",
        "url": "{{{core}}}/spaces/{{id}}/count/"
      }
    },
    "events": {
      "list": {
        "method": "GET",
        "url": "{{{core}}}/events/",
        "qs": {
          "start_time": "{{start_time}}",
          "end_time": "{{end_time}}",
          "space_id": "{{id}}",
          "doorway_id": "{{doorway_id}}"
        }
      }
    },
    "resets": {
      "list": {
        "method": "GET",
        "url": "{{{core}}}/resets/",
        "qs": {
          "start_time": "{{start_time}}",
          "end_time": "{{end_time}}",
          "space_id": "{{id}}"
        }
      }
    },
    "sensors": {
      "list": {
        "method": "GET",
        "url": "{{{core}}}/sensors/",
        "qs": {
          "start_time": "{{start_time}}",
          "end_time": "{{end_time}}",
          "space_id": "{{id}}"
        }
      },
      "get": {
        "method": "GET",
        "url": "{{{core}}}/sensors/{{id}}"
      },
      "delete": {
        "method": "DELETE",
        "url": "{{{core}}}/sensors/{{id}}"
      }
    }
  }
}
